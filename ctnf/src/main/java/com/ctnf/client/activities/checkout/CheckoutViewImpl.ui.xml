<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
    xmlns:b.html="urn:import:org.gwtbootstrap3.client.ui.html">
    
    <ui:style>
      .centralize {
			text-align:center;
			display: block;
    		margin-left: auto;
    		margin-right: auto
		}
	</ui:style>
    
    
    <b:Container fluid="true">
    	
    	
    	<b:Row marginTop="20"> 
    	
    		<b:Column size="XS_10,SM_10,MD_8,LG_8" offset="XS_1,SM_1,MD_2,LG_2">
		        	
<!-- 		     <b:Well size="LARGE">	 -->
		        
		        <b:Image addStyleNames="{style.centralize}" ui:field="imageLogoCompareTicketsNow" responsive="true"/>
			    
			      <b:Row>
    	
			        	<b:Column size="XS_10,SM_10,MD_10,LG_10" offset="XS_1,SM_1,MD_1,LG_1">
				        	
				        	<b.html:Paragraph addStyleNames="{style.centralize}" ui:field="paragraph" emphasis="PRIMARY"/>
			        		
			        	</b:Column>
		        	         	
		    		</b:Row>
		    	
		    	<b:Row>
		    	
		        	<b:Column size="XS_2,SM_2,MD_2,LG_2" offset="XS_5,SM_5,MD_5,LG_5">
		
			        	<b.html:Paragraph addStyleNames="{style.centralize}" emphasis="PRIMARY">	        	
			        		<b:Icon type="SPINNER" size="TIMES3" spin="true"/>
		    			</b.html:Paragraph>
		    			
		        	</b:Column>
		        	         	
		    	</b:Row>
			        
<!-- 			 </b:Well> -->
			    			        	
		     </b:Column>
		      
        	
    	</b:Row>    	
    	
    </b:Container>
 
 
</ui:UiBinder>
